<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mavigation"
    app:startDestination="@id/checklistFragment">
    <fragment
        android:id="@+id/checklistFragment"
        android:name="com.subzero.coviddiary.ChecklistFragment"
        android:label="ChecklistFragment"
        tools:layout="@layout/fragment_checklist">
        <action
            android:id="@+id/action_checklistFragment_to_addEntryFragment"
            app:destination="@id/addEntryFragment" />
    </fragment>
    <fragment
        android:id="@+id/addEntryFragment"
        android:name="com.subzero.coviddiary.AddEntryFragment"
        tools:layout="@layout/fragment_add_entry"
        android:label="AddEntryFragment" />
</navigation>